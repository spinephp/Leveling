<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_welcom"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    android:windowTranslucentNavigation="false"
    android:background="@drawable/welcome2"
    tools:context="com.leveling.WelcomActivity">

    <EditText
        android:id="@+id/pre_login_name"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text=""/>
    <EditText
        android:id="@+id/pre_login_pwd"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text=""/>
    <LinearLayout
        android:layout_alignParentRight="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="right"
        android:layout_marginRight="@dimen/x80"
        android:layout_marginTop="@dimen/x40"
        android:layout_alignParentTop="true"
        android:padding="4dp"
        android:background="@drawable/jump_button_border"
        android:orientation="horizontal"
        android:id="@+id/linearLayout2">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="跳过("/>

        <TextView
            android:id="@+id/djs"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="3"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="s)"
            android:id="@+id/textView" />
    </LinearLayout>

    <TextView
        android:id="@+id/btn_jump"
        android:visibility="gone"
        android:layout_marginBottom="27dp"
        android:background="@drawable/jump"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="18dp"
        android:layout_marginEnd="18dp"
        android:layout_alignParentBottom="true"
        android:layout_alignRight="@+id/linearLayout2"
        android:layout_alignEnd="@+id/linearLayout2" />
</RelativeLayout>
